<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:ns1="http://schemas.android.com/tools">
    <dimen name="cancel_icon_height">16dp</dimen>
    <dimen name="cancel_icon_width">16dp</dimen>
    <dimen name="cancel_payment_width">48dp</dimen>
    <string name="textview_disclaimer_save_card_header_title">Enter Card Details</string>
    <style name="GoSellSDKAppTheme" parent="Theme.AppCompat.Light">
        <item name="android:statusBarColor">@color/black</item>
        <item name="actionBarStyle">@style/GoSellSDKActionBar</item>
    </style>
    <style name="GoSellSDKAppTheme.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
        <item name="android:statusBarColor">@color/french_gray</item>

    </style>
    <style name="GoSellSDKAppTheme_Full" parent="GoSellSDKAppTheme">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
        <item name="android:statusBarColor" ns1:ignore="NewApi">@color/french_gray</item>
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>
    <style name="SecurityText">
        <item name="android:textColor">@color/security_text_color</item>
        <item name="android:textSize">@dimen/font_size_13sp</item>
        <item name="android:textStyle">normal</item>
        <item name="android:fontFamily" ns1:targetApi="jelly_bean">@font/avenir_next_regular
        </item>
        <item name="android:lineSpacingExtra">@dimen/title_lineSpacingExtra_0</item>
    </style>
    <style name="TransparentCompat" parent="@style/Theme.AppCompat.Light.NoActionBar">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:colorBackgroundCacheHint">@null</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowAnimationStyle">@android:style/Animation</item>
    </style>
    <style name="popup_window_animation_phone">
        <item name="android:windowEnterAnimation">@android:anim/fade_in</item>
    </style>
</resources>